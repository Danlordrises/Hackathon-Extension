{
  "manifest_version": 2,
  "name": "Movie Aficionado", 
  "description": "Movie Quote Extension",
  "version": "1.0",
  "author": "Dan Nguyen and Kirsten Coronado",
  "browser_action": {
   "default_icon": "ext-icon2.png"
  },
  
  "chrome_url_overrides" : {
    "newTab": "tab.html"
  },

  "permissions": ["storage", "tabs", "https://api.hackernoon.com"],

  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}